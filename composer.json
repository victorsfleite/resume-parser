    "resume",
    "parser"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Leite",
      "email": "victorsfleite@gmail.com"
    }
  ],
  "require": {
    "smalot/pdfparser": "^0.14.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "^0.9.8"
  },
  "autoload": {
    "psr-4": {
